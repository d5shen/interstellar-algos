{
    "name": "interstellar-algos",
    "version": "0.1.0",
    "description": "",
    "scripts": {
        "codegen": "typechain --target ethers-v5 --outDir ./types/ethers ./node_modules/@perp/contract/build/contracts/**/*.json",
        "compile": "tsc -b .",
        "compile-watch": "tsc -b --watch .",
        "build": "npm run codegen && npm run compile",
        "clean": "rm -rf ./build ./.serverless ./nyc_output",
        "index": "env $(cat .env.production | grep -v '#') npx ts-node --files src/index.ts",
        "invoke-local": "sls invoke local --stage production --function arbitrageur",
        "deploy": "npm run build && sls deploy --aws-s3-accelerate --stage production",
        "fast-deploy": "npm run compile && sls deploy --aws-s3-accelerate --stage production"
    },
    "author": "",
    "license": "BSD-3-Clause",
    "dependencies": {
        "@perp/contract": "1.0.4",
        "@types/dateformat": "^3.0.1",
        "async-mutex": "0.2.6",
        "buffer-crc32": "^0.2.13",
        "cli-table": "^0.3.6",
        "dateformat": "^4.5.1",
        "ethers": "^5.0.19",
        "fast-crc32c": "^2.0.0",
        "ftx-api": "^1.0.13",
        "ftx-api-rest": "0.0.6",
        "heap-js": "^2.1.5",
        "lodash": "4.17.15",
        "log4js": "6.2.1",
        "luxon": "1.23.0",
        "nan": "^2.14.2",
        "node-fetch": "2.6.0",
        "reflect-metadata": "0.1.13",
        "source-map-support": "0.5.19",
        "typedi": "0.8.0",
        "uuid": "^8.3.2",
        "yaml": "^1.10.2",
        "zeromq": "^5.2.8"
    },
    "devDependencies": {
        "@testdeck/di-typedi": "0.0.10",
        "@typechain/ethers-v5": "3.0.0",
        "@types/big.js": "^4.0.5",
        "@types/lodash": "4.14.154",
        "@types/luxon": "1.22.0",
        "@types/node": "13.13.4",
        "@types/node-fetch": "2.5.7",
        "@types/zeromq": "^5.2.0",
        "aws-sdk": "2.667.0",
        "big.js": "^6.1.1",
        "dotenv": "8.2.0",
        "serverless": "1.69.0",
        "serverless-dotenv-plugin": "2.4.2",
        "serverless-offline": "6.1.4",
        "serverless-webpack": "5.3.2",
        "ts-node": "8.9.1",
